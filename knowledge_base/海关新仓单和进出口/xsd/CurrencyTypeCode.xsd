<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="CurrencyTypeCode" targetNamespace="CurrencyTypeCode" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--******金额类型代码定义文档******-->
	<!--请参照《中国海关舱单数据模型代码集》-->
	<!--代码表编号：CN025。-->
	<!--该代码长度：3位，由字母与数字组成。-->
	<xs:simpleType name="CurrencyTypeCode">
		<xs:annotation>
			<xs:documentation>金额类型代码</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="3"/>
			<xs:pattern value="[A-Za-z0-9]+"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
