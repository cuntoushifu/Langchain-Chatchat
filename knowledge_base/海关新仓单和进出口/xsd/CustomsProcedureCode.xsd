<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="CustomsProcedureCode" targetNamespace="CustomsProcedureCode" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--******海关手续代码定义文档******-->
	<!--请参照《中国海关舱单数据模型代码集》-->
	<!--代码表编号：CC003。-->
	<!--该代码最大长度：7位，由字母与数字组成。-->
	<xs:simpleType name="CustomsProcedureCode">
		<xs:annotation>
			<xs:documentation>海关手续代码</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="7"/>
			<xs:pattern value="([A-Za-z0-9]+)"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
