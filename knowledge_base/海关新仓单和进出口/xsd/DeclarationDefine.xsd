	<xs:import namespace="TransportEquipmentCharacteristicText" schemaLocation="..\Code\TransportEquipmentCharacteristicText.xsd"/>
	<!--******重箱或者空箱标识代码定义文档******-->
	<xs:import namespace="TransportEquipmentFullnessCode" schemaLocation="..\Code\TransportEquipmentFullnessCode.xsd"/>
	<!--******集装箱来源代码定义文档******-->
