			<!--航班号-->
			<xs:element name="FLIGHT_DATE" type="yyyyMMdd">
				<xs:annotation>
					<xs:documentation>UNTDED 2280 Departure date (and time). 航班日期</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--航班日期-->
			<xs:element name="FROM_AIRPORT" type="untded3215">
				<xs:annotation>
					<xs:documentation>UNTDED 3215 Place of departure, coded. 出发港</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--出发港-->
			<xs:element name="TO_AIRPORT" type="untded3259">
				<xs:annotation>
					<xs:documentation>UNTDED 3259 Place of destination, coded. 目的港</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--航空港-->
