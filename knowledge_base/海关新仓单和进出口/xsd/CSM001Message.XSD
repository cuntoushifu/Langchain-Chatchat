				<xs:simpleType>
					<xs:restriction base="X8">
						<xs:pattern value="([12]\d{3}((0[13578]|1[02])(0[1-9]|[12]\d|3[01])|(0[469]|11)(0[1-9]|[12]\d|30)|02(0[1-9]|1\d|2[0-8]))|[12]\d([02468][048]|[13579][26])(02(0[1-9]|[12]\d)))?"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BuildDate">
