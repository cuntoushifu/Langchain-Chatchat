			<xs:element name="VesselNameEn">
				<xs:annotation>
					<xs:documentation>160 Identification of means of transport crossing the border 船名</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="wco160">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="VesselNameCn" type="wco160">
				<xs:annotation>
					<xs:documentation>160 Identification of means of transport crossing the border 中文船名</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IMO" type="wco167">
				<xs:annotation>
					<xs:documentation>167 Identification of means of transport crossing the border, coded IMO号码</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CallSign" type="wco167">
				<xs:annotation>
					<xs:documentation>167 Identification of means of transport crossing the border, coded 船舶呼号</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Nationality" type="wco175">
