<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="CountryCode" targetNamespace="CountryCode" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--******国家代码定义文档******-->
	<!--请参照《中国海关舱单数据模型代码集》-->
	<!--代码表编号：CN001。-->
	<!--该代码最大长度：3位，由字母和数字组成。-->
	<xs:simpleType name="CountryCode">
		<xs:annotation>
			<xs:documentation>国家代码</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="2"/>
			<xs:maxLength value="3"/>
			<xs:pattern value="[A-Za-z0-9]+"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
