<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="CustomsCode" targetNamespace="CustomsCode" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--******中国海关关区代码定义文档******-->
	<!--请参照《中国海关舱单数据模型代码集》-->
	<!--代码表编号：CC011-->
	<!--该代码长度：4位，由字母与数字组成。-->
	<xs:simpleType name="CustomsCode">
		<xs:annotation>
			<xs:documentation>中国海关关区代码</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="4"/>
			<xs:pattern value="[0-9]{2}[A-Za-z0-9]+"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
